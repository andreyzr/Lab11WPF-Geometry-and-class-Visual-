<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Path Fill="Red" Stroke="Black">
            <Path.Data>
                <!--<LineGeometry StartPoint="100,20" EndPoint="200,60"/>-->
                <RectangleGeometry Rect="100,20,100,50"/>
            </Path.Data>
        </Path>
        <Path Fill="DeepPink" Stroke="Black">
            <Path.Data>
                <RectangleGeometry Rect="100,20,100,50" RadiusX="10" RadiusY="10"/>
            </Path.Data>
        </Path>
        <Path Fill="Yellow" Stroke="Black">
            <Path.Data>
                <EllipseGeometry  RadiusX="50" RadiusY="25" Center="150,40"/>
            </Path.Data>
        </Path>
        <Path Fill="Gray" Stroke="Black">
            <Path.Data>
                <GeometryGroup FillRule="Nonzero">
                    <EllipseGeometry  RadiusX="50" RadiusY="25" Center="130,40"/>
                    <EllipseGeometry  RadiusX="50" RadiusY="25" Center="170,40"/>
                </GeometryGroup>
            </Path.Data>
        </Path>
    </StackPanel>
</Window>
